&FORCE_EVAL 
	METHOD QS
	&SUBSYS 
		&CELL 
			A 13.397596 0.0 7.735104
			B 4.465864 12.631372 7.735104
			C 0.0 0.0 15.470208
		&END 
		&KIND Si
			ELEMENT Si
			MAGNETIZATION 0.0
			BASIS_SET DZVP-MOLOPT-GTH
			POTENTIAL GTH-PBE
		&END 
	&END 
	&DFT 
		BASIS_SET_FILE_NAME BASIS_MOLOPT
		POTENTIAL_FILE_NAME GTH_POTENTIALS
		UKS True
		MULTIPLICITY 0
		CHARGE 0
		&QS 
			METHOD GPW
			EPS_DEFAULT 1e-06
			EXTRAPOLATION ASPC
		&END 
		&SCF 
			MAX_SCF 20
			EPS_SCF 1e-07
			SCF_GUESS RESTART
			&OT 
				MINIMIZER DIIS
				PRECONDITIONER FULL_SINGLE_INVERSE
				ENERGY_GAP 0.01
			&END 
			&OUTER_SCF 
				MAX_SCF 20
				EPS_SCF 1e-07
			&END 
		&END 
		&MGRID 
			CUTOFF 1200 ! Cutoff in [Ry] for finest level of the MG.
			REL_CUTOFF 80 ! Controls which gaussians are mapped to which level of the MG
			NGRIDS 5
			PROGRESSION_FACTOR 3
		&END 
		&XC 
			&XC_FUNCTIONAL PBE
			&END 
		&END 
		&PRINT 
			&PDOS 
				NLUMO -1
				COMPONENTS 
				&LDOS 
					COMPONENTS 
				&END 
			&END 
		&END 
	&END 
&END 
&GLOBAL 
	PROJECT_NAME CP2K
	RUN_TYPE DEBUG
&END 
